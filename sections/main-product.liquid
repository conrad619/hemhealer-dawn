  //start adding span for rc label
  function insertSpanLabel() {
    // Find the element with the price text (assuming it has the class 'rc-radio__label')
    var labelOriginal = document.querySelector('.rc-radio__label');
    var labelElement = null;
    var labelText = null;
    if (!document.querySelector('.rc-radio__label-clone')) {
      labelElement = labelOriginal.cloneNode(true);
      // document.querySelector('.rc-radio__label').style.display = 'none';
      labelElement.classList.remove('rc-radio__label');
      labelElement.classList.add('rc-radio__label-clone');
      // Get the text content of the element
      labelText = labelElement.textContent;
    } else {
      labelElement = document.querySelector('.rc-radio__label-clone');

      labelText = labelOriginal.textContent;
    }

    // Use a regular expression to match the price (e.g., "$199.99")
    var priceRegex = /\$\d+(\.\d{2})?/;

    // Find the price in the text
    var priceMatch = labelText.match(priceRegex);

    // If a price is found, surround it with a span
    if (priceMatch) {
      var price = priceMatch[0]; // Extract the matched price

      // Create a new span element to wrap the price
      if (!document.querySelector('.rc-radio__label-clone')) {
        var priceSpan = document.createElement('span');
        priceSpan.classList.add('rc-radio__price');
        priceSpan.textContent = price;
        var priceTextSpan = document.createElement('span');
        priceTextSpan.classList.add('rc-radio__text');
        priceTextSpan.textContent = 'Pay full price.';
        // Replace the price in the original text with the span-wrapped price
        labelElement.innerHTML = labelText.replace(price, priceSpan.outerHTML);
        labelOriginal.insertAdjacentElement('afterend', labelElement);
        labelElement.append(priceTextSpan);
      } else if (document.querySelector('.rc-radio__price')) {
        document.querySelector('.rc-radio__price').textContent = price;
      }
    }
  }

  //end adding span for rc label end
  function handleRechargeV3(callback = null) {
    console.log('vcharge');
    const radioButtons = document.querySelectorAll('[name="purchaseOption"]');
    if (!radioButtons.length) return false;

    radioButtons.forEach((radio) => {
      radio.addEventListener('change', () => {
        const isSubscription = radio.value === 'subscription' && radio.checked;
        insertSubscriptionDescription();
        insertSpanLabel();
      });

      // Initial check on load
      if (radio.checked && radio.value === 'subscription') {
        insertSubscriptionDescription();
        insertSpanLabel();
      }
    });

    document.addEventListener('change', function (e) {
      if (e.target.closest('variant-selects') || e.target.closest('.product-form')) {
        setTimeout(() => {
          insertSpanLabel();
        }, 100); // Slight delay to allow DOM to update
      }
    });

    if (callback != null) {
      callback();
    }

    return true;
  }

  function elementExist(element, maxAttempts = 10, intervalTime = 500) {
    let attempts = 0;

    const intervalId = setInterval(() => {
      const elementExist = element();

      if (elementExist) {
        // console.log('Element found:', element);
        clearInterval(intervalId); // Stop further attempts
      } else {
        attempts++;
        if (attempts >= maxAttempts) {
          // console.warn('Element not found after max attempts.');
          clearInterval(intervalId); // Stop after 10 tries
        }
      }
    }, intervalTime);
  }

  elementExist(handleRechargeV3, 30);

